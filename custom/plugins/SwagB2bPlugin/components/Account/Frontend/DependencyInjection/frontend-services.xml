<?xml version="1.0"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
  <parameters>
    <parameter key="b2b_account.controller_class">Shopware\B2B\Account\Frontend\AccountController</parameter>
  </parameters>
  <services>
    <service id="b2b_account.controller_abstract" abstract="true">
      <argument type="service" id="b2b_front_auth.authentication_service"/>
      <argument type="service" id="b2b_account.service"/>
      <argument type="service" id="b2b_shop.storage"/>
      <argument type="service" id="b2b_account.image_service"/>
      <argument type="service" id="b2b_front_auth.auth_storage_adapter"/>
    </service>
    <service id="b2b_account.controller" class="%b2b_account.controller_class%" parent="b2b_account.controller_abstract"/>
  </services>
</container>
