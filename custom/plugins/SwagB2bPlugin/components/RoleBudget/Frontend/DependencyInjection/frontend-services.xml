<?xml version="1.0"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
  <parameters>
    <parameter key="b2b_role_budget.controller_class">Shopware\B2B\RoleBudget\Frontend\RoleBudgetAssignmentController</parameter>
  </parameters>
  <services>
    <service id="b2b_role_budget.controller_abstract" abstract="true">
      <argument type="service" id="b2b_front_auth.authentication_service"/>
      <argument type="service" id="b2b_budget.repository"/>
      <argument type="service" id="b2b_budget.acl_repository"/>
      <argument type="service" id="b2b_role.repository"/>
      <argument type="service" id="b2b_budget.grid_helper"/>
      <argument type="service" id="b2b_acl.access_extension_service"/>
      <argument type="service" id="b2b_currency.service"/>
      <argument type="service" id="b2b_role_budget.crud_service"/>
    </service>
    <service id="b2b_role_budget.controller" class="%b2b_role_budget.controller_class%" parent="b2b_role_budget.controller_abstract"/>
  </services>
</container>
