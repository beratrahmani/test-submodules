<?xml version="1.0" encoding="utf-8"?>
<cronjobs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="../../../../engine/Shopware/Components/Plugin/schema/cronjob.xsd">

    <cronjob>
        <name>B2B Budget Notify Author</name>
        <action>B2bBudgetNotifyAuthor</action>
        <active>true</active>
        <interval>86400</interval>
        <disableOnError>true</disableOnError>
    </cronjob>
    <cronjob>
        <name>B2B Order Trigger Queue</name>
        <action>B2bOrderTriggerQueue</action>
        <active>true</active>
        <interval>3600</interval>
        <disableOnError>true</disableOnError>
    </cronjob>
</cronjobs>